<!doctype html>
<html>
<head>
<meta charset="utf-8">
<title>Grade Assignment</title>
<script>
/*
     Dany Dang
	 Sept 9th, 2015
	 Purpose:  To create grading program.
*/
var grade = prompt("What's your score?");
if (grade<=60){
	document.write("Your grade is an F");
	}
	else if (grade<=65){
	document.write("Your grade is a D");
	}
	else if (grade<=75){
	document.write("Your grade is a C");
	}
	else if (grade<=85){
	document.write("Your grade is a B");
	}
	else if (grade<=100){
	document.write("Your grade is an A");
	}
</script>
</head>

<body>
<script>
</script>
</body>
</html>